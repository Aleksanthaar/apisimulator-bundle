{
    "name": "aleksanthaar/apisimulator-bundle",
    "description": "Simlet generator integrated to Symfony profiler",
    "type": "library",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "alhuo-smile",
            "email": "alexandre.huon@smile.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "Aleksanthaar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Aleksanthaar\\": "tests/"
        }
